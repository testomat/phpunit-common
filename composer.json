{
    "name": "testomat/phpunit-common",
    "type": "phpunit-printer",
    "description": ":crystal_ball: Find and report on slow tests in your PHPUnit test suite",
    "keywords": [
        "narrowspark",
        "testomat",
        "phpunit",
        "hook",
        "speedtrap"
    ],
    "homepage": "http://github.com/testomat/phpunit",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "thecodingmachine/safe": "^1.1.0"
    },
    "require-dev": {
        "testomat/terminal-colour": "^1.1.0"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Testomat\\PHPUnit\\SpeedTrap\\": ""
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/testomat/phpunit/issues",
        "source": "https://github.com/testomat/phpunit"
    },
    "suggest": {
        "phpunit/phpunit": "Required to use this printer (^8.0.0 || ^9.0.0)."
    }
}
